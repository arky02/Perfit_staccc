<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="300dp"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    android:layout_gravity="center"
    android:background="@drawable/dialog_design">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="276dp"
        android:orientation="vertical"
        android:gravity="center">


        <LinearLayout
            android:layout_width="276dp"
            android:layout_height="276dp"
            android:orientation="vertical"
            android:gravity="center"
            android:visibility="visible">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:fontFamily="@font/nanumsquareeb"
                android:textColor="#424040"
                android:textSize="20sp"
                android:text="새로운 모델의" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/nanumsquareeb"
                android:textColor="#424040"
                android:textSize="20sp"
                android:text="신체 치수 측정을 위해" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#424040"
                android:textSize="20sp"
                android:text="모델 사진 촬영을 진행합니다"
                android:fontFamily="@font/nanumsquareeb"/>


            <Button
                android:id="@+id/btn_ok33"
                android:layout_width="124dp"
                android:layout_height="38.8dp"
                android:text="OK"
                android:textStyle="bold"
                android:textSize="20sp"
                android:layout_marginTop="45dp"
                android:textColor="#ffffff"
                android:background="@drawable/buttondesign3"/>

        </LinearLayout>

    </LinearLayout>
</LinearLayout>